(*funciones*)
MODULE seis;

  VAR resta:INTEGER;
  		a1:INTEGER;
  		b1:INTEGER;
  
  PROCEDURE Restar (a: INTEGER; b: INTEGER) : INTEGER;
  	VAR s: INTEGER;
 	BEGIN
 		s := a-b;
 		RETURN s; 	
 	END Restar; 	
  
  BEGIN
    a1:=10;
    b1:=5;
    
    resta := Restar(a1,b1);
    WRITESTRING("Restar:");
    WRITEINT(resta);
    
  END seis;
